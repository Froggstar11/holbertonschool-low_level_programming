 * @h: a pinter of struct list_t type.
 * Return: a size_t.
 */

size_t print_list(const list_t *h)
{
int i;
for (i = 0; h != NULL; i++)
{
if(h->str == NULL)
printf("[0] (nil)\n");
else
printf("[%u] %s\n", h->len, h->str);
h = h->next;
}
return (lenght);
}
